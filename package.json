{
	"name": "@fast-china/utils",
	"version": "1.0.34",
	"description": "Fast 工具库.",
	"type": "module",
	"keywords": [
		"fast-china",
		"fast",
		"typescript",
		"utils"
	],
	"license": "Apache-2.0",
	"homepage": "https://fastdotnet.com",
	"bugs": {
		"url": "https://gitee.com/FastDotnet/Fast.Utils/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/FastDotnet/Fast.Utils"
	},
	"author": "小方",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"check": "vue-tsc",
		"update:version": "tsx scripts/update-version.ts",
		"build": "tsx scripts/build.ts && vue-tsc && vite build --config vite.umd-iife.config.ts && vite build --config vite.es-lib.config.ts && tsx scripts/move-file.ts",
		"publish": "cd @fast-china/utils && npm publish",
		"lint:eslint": "eslint --fix",
		"lint:prettier": "prettier --write '{src,types}/**/*.{vue,js,ts,tsx,json,css,less,scss,html,md}'",
		"lint": "npm run lint:eslint && npm run lint:prettier"
	},
	"dependencies": {
		"@types/crypto-js": "^4.2",
		"@types/lodash": "^4.17",
		"@types/lodash-es": "^4.17",
		"crypto-js": "^4.2",
		"lodash": "^4.17",
		"lodash-es": "^4.17",
		"lodash-unified": "^1"
	},
	"devDependencies": {
		"@dcloudio/types": "^3",
		"@fast-china/eslint-config": "^1",
		"@rollup/plugin-terser": "^0.4",
		"@types/node": "^24",
		"@vue/tsconfig": "^0.8",
		"eslint": "^9",
		"prettier": "^3",
		"terser": "^5",
		"typescript": "^5",
		"vite": "^7",
		"vite-plugin-dts": "^4",
		"vue": "^2 || ^3",
		"vue-tsc": "^3"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
